                const menu = document.querySelector("#menu");
                menu.addEventListener("mouseover", (e) => {
                    $('.svg_comp').css('fill', '#008B9F');
                    $('#path-menu').show();
                })

                menu.addEventListener("mouseleave", (e) => {
                    $('.svg_comp').css('fill', '#C4C4C4');
                    $('#path-menu').hide();
